    <element name="M4P_Property" substitutionGroup="gml:AbstractGML" type="cim:M4P_PropertyType">
        <annotation>
            <documentation>A property used by a numerical process. This may include both simulated properties and properties that are used to perform the simulation, but that are not simulated by the process (for example, gravity).</documentation>
        </annotation>
    </element>
    <complexType name="M4P_PropertyType">
        <complexContent>
            <extension base="gml:AbstractGMLType">
                <sequence>
                    <element maxOccurs="1" minOccurs="0" name="actualValue" type="string">
                        <annotation>
                            <documentation>The actual value used for the property.  This would be populated for configured models.</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="unbounded" minOccurs="0" name="possibleValue" type="cim:M4P_PossiblePropertyValuePropertyType">
                        <annotation>
                            <documentation>A set of possible values for the property.  If null, then any value is possible.  This would be populated for unconfigured models.</documentation>
                        </annotation>
                    </element>
                    <element name="represents" type=""/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="M4P_PropertyPropertyType">
        <sequence minOccurs="0">
            <element ref="cim:M4P_Property"/>
        </sequence>
        <attributeGroup ref="gml:AssociationAttributeGroup"/>
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
    </complexType>
    <element name="M4P_TimeLag" substitutionGroup="gml:AbstractGML" type="cim:M4P_TimeLagType">
        <annotation>
            <documentation>The coupling field used in the target at a given time corresponds to a field produced by the source at a previous time. This lag specifies the difference in time.</documentation>
        </annotation>
    </element>
    <complexType name="M4P_TimeLagType">
        <complexContent>
            <extension base="gml:AbstractGMLType">
                <sequence>
                    <element maxOccurs="1" minOccurs="0" name="value" type="integer"/>
                    <element maxOccurs="1" minOccurs="0" name="units" type="cim:M4C_TimeUnitTypeType"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="M4P_TimeLagPropertyType">
        <sequence minOccurs="0">
            <element ref="cim:M4P_TimeLag"/>
        </sequence>
